{
  "name": "tragiccode-ravendb",
  "version": "0.2.0",
  "author": "tragiccode",
  "summary": "The `ravendb` module installs and manages Ravendb on Windows systems.",
  "license": "Apache-2.0",
  "source": "https://github.com/TraGicCode/tragiccode-ravendb",
  "project_page": "https://github.com/TraGicCode/tragiccode-ravendb",
  "issues_url": "https://github.com/TraGicCode/tragiccode-ravendb/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": "4.x"
    },
    {
      "name": "puppetlabs-dsc",
      "version_requirement": "1.x"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "windows",
      "operatingsystemrelease": [
        "2012",
        "2012R2",
        "2016"
      ]
